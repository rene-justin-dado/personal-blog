{
  "name": "personal-blog",
  "version": "0.1.0",
  "engines": {
    "node": "6.9.2"
  },
  "description": "Justin's Personal Blog",
  "scripts": {
    "postinstall": "webpack --optimize-minimize",
    "start": "node ./src/server/server.js",
    "nodemon": "nodemon --inspect=9228 ./src/server/server.js",
    "build": "webpack --watch",
    "devserver": "webpack-dev-server --hot --inline --open --content-base src/public",
    "sass": "node_modules/.bin/node-sass src/public/styles/main.scss -o src/public/styles/"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "EDA",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "sass-loader": "^4.1.1",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "main": "webpack.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rene-justin-dado/rj-dev.git"
  },
  "bugs": {
    "url": "https://github.com/rene-justin-dado/rj-dev/issues"
  },
  "homepage": "https://github.com/rene-justin-dado/rj-dev#readme",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "express": "^4.14.0",
    "prop-types": "^15.7.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "superagent": "^2.3.0",
    "webpack": "^1.15.0"
  }
}
